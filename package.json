{
  "name": "mcp-server-gemini",
  "version": "4.2.2",
  "description": "State-of-the-art MCP server for Google Gemini API with all latest features",
  "main": "dist/enhanced-stdio-server.js",
  "bin": {
    "mcp-server-gemini": "./dist/enhanced-stdio-server.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build && husky install",
    "start": "node dist/enhanced-stdio-server.js",
    "dev": "ts-node src/enhanced-stdio-server.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "google-gemini",
    "ai",
    "llm",
    "claude-desktop",
    "cursor",
    "windsurf",
    "typescript"
  ],
  "author": "Ali Argun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliargun/mcp-server-gemini.git"
  },
  "bugs": {
    "url": "https://github.com/aliargun/mcp-server-gemini/issues"
  },
  "homepage": "https://github.com/aliargun/mcp-server-gemini#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
